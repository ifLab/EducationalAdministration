<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hcjcch.educationaladministration.educational" >

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE">

    </uses-permission>
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver
            android:name="com.hcjcch.educationaladministration.broadcast.NetConnectionChangeReceiver"
            android:label="NetworkConnection">
            <intent-filter>
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE">

                </action>
            </intent-filter>
        </receiver>
        <activity android:name="com.hcjcch.educationaladministration.activity.OauthActivity">

        </activity>

        <!-- 空闲教室 -->
        <activity android:name="com.hcjcch.educationaladministration.educational.SchoolPlaceActivity"></activity>

    </application>

</manifest>
